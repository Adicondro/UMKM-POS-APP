generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int    @id @default(autoincrement())
  email     String @unique
  telnumber String @unique
  password  String
  role      Role   @default(OWNER)
}

enum Role {
  OWNER
  EMPLOYEE
}
